<div class="container white">
{{#if controller.content}}
{{#each anime in controller.content}}
	<div class="pull-left margin">
		<div class="animethumbnail">
			<div class="animethumbnail-fav"></div>
			<a href="#" {{action goToAnime anime target="parentView"}}><img class="animethumbnail-inner" {{bindAttr src="anime.fanartURLSmall"}}></a>
		</div>
		<p>{{anime.title}}</p>
	</div>
{{/each}}
{{/if}}
</div>